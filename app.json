{
  "expo": {
    "name": "SnapShop",
    "slug": "SnapShop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/SnapShopApp.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#055183"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}